{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "object-curly-spacing": 0,
    "quotes": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-comment-textnodes": 0,
    "react/prop-types": 0,
    "jsx-a11y/media-has-caption": 0
  }
}
